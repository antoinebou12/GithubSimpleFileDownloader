{
  "name": "Github single file downloader",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Can clone a file with the click of a button that appear to the right side of the list",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "96": "icons/icon96.png",
    "120": "icons/icon120.png",
    "144": "icons/icon144.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "contentSettings",
    "fileBrowserHandler",
    "idle",
    "notifications"
  ],
  "omnibox": {
    "keyword": "extensionizr"
  },
  "web_accessible_resources": [
    "icons/*.png",
    "src/html/error.txt"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}